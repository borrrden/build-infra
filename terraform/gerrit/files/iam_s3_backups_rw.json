{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "BackupsRW",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::cb-gerrit.backups",
                "arn:aws:s3:::cb-gerrit.backups/*"
            ]
        }
    ]
}
